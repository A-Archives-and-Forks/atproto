{
  "lexicon": 1,
  "id": "com.atproto.label.subscribeLabels",
  "defs": {
    "main": {
      "type": "subscription",
      "description": "Subscribe to label updates",
      "parameters": {
        "type": "params",
        "properties": {
          "cursor": {
            "type": "integer",
            "description": "The last known event to backfill from."
          }
        }
      },
      "message": {
        "schema": {
          "type": "object",
          "required": ["seq", "labels"],
          "nullable": ["prev"],
          "properties": {
            "seq": {"type": "integer"},
            "labels": {
              "type": "array",
              "items": { "type": "ref", "ref": "com.atproto.label.label" }
            }
          }
        }
      },
      "infos": [
        {"name": "OutdatedCursor"}
      ],
      "errors": [
        {"name": "FutureCursor"}
      ]
    }
  }
}
